<p id="notice"><%= notice %></p>

<div class="get-start-area">
                        <!-- Form Start -->
                        <form action="#" method="post" class="form-inline">
                            <input type="email" class="form-control email" placeholder="name@company.com">
                            <input type="submit" class="submit" value="Get Started">
                        </form>
                        <!-- Form End -->
                    
<h1>Testpages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testpages.each do |testpage| %>
      <tr>
        <td><%= testpage.name %></td>
        <td><%= testpage.place %></td>
        <td><%= link_to 'Show', testpage %></td>
        <td><%= link_to 'Edit', edit_testpage_path(testpage) %></td>
        <td><%= link_to 'Destroy', testpage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
<%= link_to 'New Testpage', new_testpage_path %>
